# Uncomment this line if you need to define a specific platform
platform :ios, '15.5'

use_frameworks!
use_modular_headers!

flutter_root = File.expand_path('..', File.dirname(__FILE__))
load File.join(flutter_root, 'flutter', 'tools', 'podhelper.rb')

target 'Runner' do
  flutter_install_all_ios_pods File.dirname(File.realpath(__FILE__))
end
